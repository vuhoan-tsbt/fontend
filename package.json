{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@react-google-maps/api": "^2.13.1",
    "@umijs/hooks": "^1.9.3",
    "@umijs/preset-react": "1.x",
    "ahooks": "^3.4.1",
    "antd": "^4.23.6",
    "antd-img-crop": "^4.2.8",
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react-infinite-scroll-component": "^6.1.0",
    "react-quill": "^1.3.5",
    "recoil": "^0.7.3",
    "umi": "^3.4.15"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.4.15",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
